{
  "name": "av2me",
  "version": "0.1.0",
  "description": "ExpressJS + ES7 Boilerplate",
  "repository": "https://github.com/AlloVince/av2me",
  "license": "MIT",
  "scripts": {
    "prestart": "npm run build",
    "start": "NODE_ENV=production node ./bin/www",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/_mocha --recursive",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --recursive",
    "dev": "nodemon --ignore 'node_modules/*' --exec babel-node ./bin/www",
    "swagger": "npm run build && node build/swagger/app.js",
    "lint": "eslint src/app.js",
    "build": "gulp babel && gulp copy-json"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "cors": "^2.7.1",
    "crc-32": "^0.4.0",
    "debug": "^2.2.0",
    "express": "~4.13.1",
    "glob": "^6",
    "jade": "~1.11.0",
    "js-yaml": "^3.5.2",
    "morgan": "~1.6.1",
    "mysql": "^2.10.2",
    "redis": "^2.4.2",
    "request": "^2.67.0",
    "sequelize": "^3.18.0",
    "serve-favicon": "~2.3.0",
    "serve-static": "^1.10.2",
    "swagger-ui": "^2.1.4"
  },
  "devDependencies": {
    "acorn": "^2.7.0",
    "acorn-es7-plugin": "^1.0.11",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "doctrine": "^1.1.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.16.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-cli": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "webpack": "^1.12.11"
  }
}
